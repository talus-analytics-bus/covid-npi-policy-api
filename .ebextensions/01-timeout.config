files:
  "/etc/nginx/conf.d/01-timeout.conf":
     mode: "000644"
     owner: root
     group: root
     content: |
       proxy_connect_timeout 60s;
       proxy_send_timeout 60s;
       proxy_read_timeout 60s;
       fastcgi_send_timeout 60s;
       fastcgi_read_timeout 60s;
container_commands:
  nginx_reload:
     command: "sudo service nginx restart"
